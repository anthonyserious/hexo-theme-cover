<nav id="main-nav">
  <ul class='inner'>
    <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
  <div class="clearfix"></div>
</nav>
<div class="clearfix"></div>
<div id="cover">
  <div class='profile'>
    <div class="logo">
      <a href="<%- config.root %>"> 
        <img src="<%- config.root %>logo.png" alt='Profile Picture' />
      </a>
    </div>
    <div id="title"><%= config.title %></div>
    <% if (config.subtitle){ %>
      <div id="subtitle"><%- config.subtitle %></div>
    <% } %>
  </div>
</div>